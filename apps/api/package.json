{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "dotenv -e .env.local drizzle-kit migrate",
    "db:generate": "dotenv -e .env.local drizzle-kit generate",
    "db:push": "dotenv -e .env.local drizzle-kit push",
    "db:pull": "dotenv -e .env.local drizzle-kit pull",
    "db:studio": "dotenv -e .env.local drizzle-kit studio",
    "db:seed": "dotenv -e .env.local tsx src/modules/database/scripts/seed.ts",
    "auth:generate": "pnpx @better-auth/cli generate --config=./src/modules/auth/config/auth.ts --output=./src/modules/database/schemas/auth.schema.ts --yes"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@bull-board/nestjs": "^6.14.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@scalar/nestjs-api-reference": "^1.0.4",
    "@thallesp/nestjs-better-auth": "^2.1.0",
    "@workspace/schemas": "workspace:*",
    "better-auth": "^1.3.34",
    "bullmq": "^5.61.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "drizzle-seed": "^0.3.1",
    "express-basic-auth": "^1.2.1",
    "helmet": "^8.1.0",
    "nestjs-zod": "^5.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "viem": "^2.39.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.12",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.17",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "drizzle-kit": "^0.31.5",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "5.5.4"
  }
}
